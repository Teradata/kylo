  "private": true,
  "description": "kylo",
  "scripts": {
    "build:webpack:prod": "webpack --profile --env.production",
    "start:webpack:dev": "webpack-dev-server --progress --profile --env.dev",
    "start:webpack:dev-tool-eval": "webpack-dev-server --progress --profile --env.dev-tool --devtool eval",
    "start:webpack:dev-tool-cheap": "webpack-dev-server --progress --env.dev-tool-cheap --devtool cheap-module-eval-source-map",
    "build:lib": "ng-packagr -p src/main/resources/static/lib/package.json",
    "build": "npm run build:webpack:prod",
    "serve": "lite-server -c bs-config.js",
    "start": "npm run start:webpack:dev-tool-cheap",
    "reinstall": "rm -rf node_modules tmp deploy dist && npm i",
    "test": "karma start karma.conf.js",
    "test:watch": "tsc -p src/main/resources/static/tsconfig.app.json && concurrently \"tsc -p src/main/resources/static/tsconfig.spec.json -w\" \"karma start karma.conf.js\""
  },
