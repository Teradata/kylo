    "http-proxy-middleware": "^0.17.4",
    "jasmine-core": "~2.5.2",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.3.0",
    "ng-packagr": "^2.4.5",
    "node-sass": "^4.9.3",
    "typescript": "~2.5"
  }
}
