    "@angular/forms": "~5.1",
    "@angular/http": "~5.1",
    "@angular/material": "~5.0",
    "@angular/platform-browser": "~5.1",
    "@angular/platform-browser-dynamic": "~5.1",
    "@angular/router": "~5.1",
