<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <artifactId>kylo-schema-discovery</artifactId>
  <packaging>pom</packaging>

  <parent>
    <groupId>com.thinkbiganalytics.kylo</groupId>
    <artifactId>kylo-core</artifactId>
    <version>0.10.1-SNAPSHOT</version>
  </parent>
  <properties>
    <plugin.license.parentRelative>../../</plugin.license.parentRelative>
  </properties>

  <modules>
    <module>schema-discovery-api</module>
    <module>schema-discovery-rdbms</module>
  </modules>

  <dependencies>
  </dependencies>

  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>${maven.compiler.plugin.version}</version>
        <configuration>
          <source>${spark.java.version}</source>
          <target>${spark.java.version}</target>
        </configuration>
      </plugin>
    </plugins>
  </build>

</project>
