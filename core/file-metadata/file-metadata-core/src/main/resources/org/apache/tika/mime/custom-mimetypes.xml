<mime-info>
  <mime-type type="application/parquet">
    <glob pattern="*.parquet"/>
    <magic priority="50">
      <match value="PAR1" type="string" offset="0"/>
    </magic>
    <sub-class-of type="application/octet-stream" />

  </mime-type>

  <mime-type type="application/avro">
    <glob pattern="*.avro"/>
    <magic priority="50">
      <match value="avro.schema" type="string" offset="0:10"/>
    </magic>
    <sub-class-of type="application/octet-stream" />

  </mime-type>


  <mime-type type="application/orc">
    <glob pattern="*.orc"/>
    <magic priority="50">
      <match value="ORC" type="string" offset="0"/>
    </magic>
    <sub-class-of type="application/octet-stream" />

  </mime-type>



</mime-info>